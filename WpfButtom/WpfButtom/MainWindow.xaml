<Window x:Class="WpfButtom.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WpfButtom"
        mc:Ignorable="d"
        Title="Buttom" Height="200" Width="200">
    <StackPanel>
        <Button Content="Buttom" Click="Button_Click"/>
        <RepeatButton Content="Repeat Buttom" Click="RepeatButton_Click" />
        <ToggleButton Content="Toggle Button" Click="ToggleButton_Click" />
        <TextBlock x:Name="meLogs"/>
        

    </StackPanel>
</Window>
